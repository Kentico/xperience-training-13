<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>PasswordConfirmator</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides two fields for entering user account passwords - one for the initial value, the second for confirmation. Masks input by displaying dots instead of the typed-in characters. Validates the input according to the site's Password policy settings (Settings -> Security & Membership -> Passwords) and can optionally display an indicator of the password strength.]]>
  </UserControlDescription>
  <UserControlDisplayName>Password with confirmation</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/Membership/FormControls/Passwords/PasswordConfirmator.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>False</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>05f6cc8d-4ed1-44d4-91d8-4c54f5c47705</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field column="showstrength" columntype="boolean" displayinsimplemode="true" guid="c4c2d1f5-a963-4370-bc07-6299011c09fd" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show password strength</fieldcaption>
          <fielddescription>
            <![CDATA[If true, the calculated strength of the password will be shown below the textbox.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>False</UserControlShowInWebParts>
</cms.formusercontrol>