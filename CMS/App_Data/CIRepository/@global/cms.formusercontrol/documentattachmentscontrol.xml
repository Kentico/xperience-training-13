<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>DocumentAttachmentsControl</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a tool for uploading files from the file system as page attachments.]]>
  </UserControlDescription>
  <UserControlDisplayName>Attachments</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSFormControls/Documents/DocumentAttachmentsControl.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>True</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>False</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>False</UserControlForText>
  <UserControlGUID>aa388dab-6d70-4f8b-83e2-144879726798</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field allowempty="true" column="ChangeOrder" columntype="boolean" guid="b5a229bf-ac59-4095-bd78-1bdfb4c98e84" spellcheck="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Allow change order</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if users should be allowed to change the order of the uploaded attachments in the form.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Paging" columntype="boolean" guid="2103ef61-6b3d-4acb-91e0-49b43bd7d04f" spellcheck="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Paging</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if paging should be used for the list of attachments.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="PagingSize" columnsize="100" columntype="text" guid="78d06e8a-e4b0-45b2-a8da-95b9e1897785" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Page size</fieldcaption>
          <fielddescription>
            <![CDATA[Defines the page size options that will be selectable in the pager of the attachment list. Values must be separated by commas. Default values are: 25,50,100,##ALL##.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="DefaultPageSize" columntype="integer" guid="f4425b1d-114c-4825-aeae-2332a0f6564f" visible="true">
        <properties>
          <fieldcaption>Default page size</fieldcaption>
          <fielddescription>Sets the default page size.</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="Extensions" columnsize="10" columntype="text" displayinsimplemode="true" guid="54336bd4-af1a-44da-9b4b-e05334b50f9c" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Allowed extensions</fieldcaption>
          <fielddescription>
            <![CDATA[Only files using the extensions specified here can be uploaded into the field. Multiple extensions must be separated by semicolons. By default, the list of allowed extensions is inherited from the Settings -> System -> Files -> Upload extensions setting.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>allowedextensionsselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Allowed_Extensions" columnsize="250" columntype="text" guid="a2588748-4241-445e-ae0c-2ed0d6d359c4" spellcheck="false">
        <settings>
          <controlname>agerangeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize" columntype="longtext" guid="c502dc06-7349-4674-aa33-d4ec06497072" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Automatic image resize on upload</fieldcaption>
          <fielddescription>
            <![CDATA[The resizing settings specified here are applied to all uploaded image-type files.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>autoresizeconfiguration</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_Width" columntype="integer" guid="16f479ec-76b4-47e4-b369-98a384925ac0" spellcheck="false">
        <settings>
          <controlname>badgeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_Height" columntype="integer" guid="20cb92a0-4117-4fe1-8328-0b856fbf33bf" spellcheck="false">
        <settings>
          <controlname>badgeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_MaxSideSize" columntype="integer" guid="7add1b70-9ab3-4dc5-a596-2e048de5cdcb" spellcheck="false">
        <settings>
          <controlname>badgeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_Hashtable" columntype="boolean" guid="0a774b3e-3452-440d-aa4e-727af8fd3bf1" spellcheck="false">
        <properties>
          <defaultvalue>true</defaultvalue>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>False</UserControlShowInSystemTables>
  <UserControlShowInWebParts>False</UserControlShowInWebParts>
</cms.formusercontrol>