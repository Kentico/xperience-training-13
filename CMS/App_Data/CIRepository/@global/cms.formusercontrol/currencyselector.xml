<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>currencySelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with available E-commerce currencies.]]>
  </UserControlDescription>
  <UserControlDisplayName>Currency selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/ECommerce/FormControls/currencyselector.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>08ffcbd5-85ad-4b09-9119-5d11d29b6456</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field column="UseCurrencyNameForSelection" columntype="boolean" guid="18d127fd-ad20-473f-975e-e56c9826e2a6" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use currency name for selection</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selector returns the code name of the selected item, instead of its ID.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="DisplayOnlyWithExchangeRate" columntype="boolean" guid="89cc471e-3410-4684-93b5-0dde4da8217e" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Display currencies with exchange rate</fieldcaption>
          <fielddescription>
            <![CDATA[If true, only currencies with exchange rate are displayed by this selector.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AllowCreate" columntype="boolean" guid="cf120254-6405-409f-a6b7-c552680f1555" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>{$com.selector.allowcreatetitle$}</fieldcaption>
          <fielddescription>{$com.selector.allowcreatedescription$}</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="EnsureSelectedItem" columntype="boolean" guid="18f00365-406d-4ad3-9f63-335a29851885" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Ensure selected item</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selected item is present in the list regardless of other settings.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddAllItemsRecord" columntype="boolean" guid="2535bdd3-642b-44da-aa5d-d95aa4423f21" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Add all items record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (all) option for selecting all currencies is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddNoneRecord" columntype="boolean" guid="5ec19d63-984b-4d8f-adef-73428dc136ba" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Add none record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (none) option is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayOnlyEnabled" columntype="boolean" guid="6d7581f9-b6f8-45b5-9980-dd208bd8c829" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display only enabled</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if only enabled currencies are to be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlResourceID>
    <CodeName>CMS.Ecommerce</CodeName>
    <GUID>684e021b-8b56-4cbf-8fd5-b7a791fd2dbc</GUID>
    <ObjectType>cms.resource</ObjectType>
  </UserControlResourceID>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>False</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>