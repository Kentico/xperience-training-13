<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>CalendarControl</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a calendar date and time picker and a 'Now' button, which allows users to choose the current date and time. ]]>
  </UserControlDescription>
  <UserControlDisplayName>Calendar</UserControlDisplayName>
  <UserControlFileName>~/CMSFormControls/Basic/CalendarControl.ascx</UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>True</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>False</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>False</UserControlForText>
  <UserControlGUID>78b921b6-d8ae-4d96-a927-c03a919c9b8c</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field allowempty="true" column="EditTime" columntype="boolean" displayinsimplemode="true" guid="0c4d3be0-a0a6-45d4-8805-e673f3181b08" spellcheck="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Edit time</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if users should be able to change the date and time in the field.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="DisplayNow" columntype="boolean" displayinsimplemode="true" guid="ee698eda-6af1-4e58-837c-f4c2f2ab58fa" spellcheck="false" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>
            <![CDATA[Show 'Now' link]]>
          </fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the 'Now' option should be displayed next to the field.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="CheckRange" columntype="boolean" guid="6b2d3718-71bb-4aad-a55b-6b317cbd2959" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Check range</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the range of the entered date/time is validated]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="TimeZone">
        <properties>
          <caption>Time zone</caption>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="TimeZoneType" columnsize="100" columntype="text" guid="5681b891-6fe4-4b5f-ae83-0626002d35bb" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Time zone type</fieldcaption>
          <fielddescription>
            <![CDATA[Allows the selection of the time zone applied to the time displayed in the field.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>timezonetypeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="TimeZone" columnsize="100" columntype="text" guid="acbd8c45-56f8-4851-b9a7-39c7f1f1899c" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Custom time zone</fieldcaption>
          <fielddescription>
            <![CDATA[If the Time zone type is set to 'Custom' then a specific time zone can be selected here.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>timezoneselector</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>100</UserControlPriority>
  <UserControlShowInCustomTables>True</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
  <UserControlShowInReports>True</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>