<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>shippingSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with available E-commerce shipping options.]]>
  </UserControlDescription>
  <UserControlDisplayName>Shipping selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/ECommerce/FormControls/shippingselector.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>baf64afb-454b-4dac-a4e7-d304e8eacaed</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field column="UseShippingNameForSelection" columntype="boolean" guid="ef9b8ead-1122-4683-ad18-ecd85081b227" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use shipping name for selection</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selector returns the code name of the selected item, instead of its ID.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="EnsureSelectedItem" columntype="boolean" guid="4957f77f-e60a-46ef-8f78-486f1bb095b0" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Ensure selected item</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selected item is present in the list regardless of other settings.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddAllItemsRecord" columntype="boolean" guid="897c63e4-5209-4d19-a7ef-d7003c265726" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Add all items record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (all) option for selecting all shippings is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddNoneRecord" columntype="boolean" guid="25aa4706-c552-460d-a822-017e5e3cd3d3" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Add none record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (none) option is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayOnlyEnabled" columntype="boolean" guid="84152e89-52a5-4321-8143-abe6af175eda" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display only enabled</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if only enabled shipping options are to be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlResourceID>
    <CodeName>CMS.Ecommerce</CodeName>
    <GUID>684e021b-8b56-4cbf-8fd5-b7a791fd2dbc</GUID>
    <ObjectType>cms.resource</ObjectType>
  </UserControlResourceID>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>False</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>