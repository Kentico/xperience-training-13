<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>EnumSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Represents a DDL selector form control which initializes its items from a specified enum type.]]>
  </UserControlDescription>
  <UserControlDisplayName>Enum selector</UserControlDisplayName>
  <UserControlFileName>~/CMSFormControls/System/EnumSelector.ascx</UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>b5902bf6-6593-4c6a-95e8-83be91d9b98b</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <category name="Enumeration">
        <properties>
          <visible>true</visible>
        </properties>
      </category>
      <field column="AssemblyName" columnsize="100" columntype="text" displayinsimplemode="true" guid="943fc2e9-a352-4be6-8ca1-988d3213ba79" visible="true">
        <properties>
          <fieldcaption>Enum assembly name</fieldcaption>
          <fielddescription>
            <![CDATA[The name of the assembly where the required enum is located.]]>
          </fielddescription>
        </properties>
        <settings>
          <ClassNameColumnName>TypeName</ClassNameColumnName>
          <controlname>assemblyclassselector</controlname>
          <ShowClasses>False</ShowClasses>
          <ShowEnumerations>True</ShowEnumerations>
          <ShowInterfaces>False</ShowInterfaces>
        </settings>
      </field>
      <field column="TypeName" columnsize="100" columntype="text" displayinsimplemode="true" guid="fe977ba9-b52c-46a6-a04a-82a09ccda24e">
        <properties>
          <fieldcaption>Enum type name</fieldcaption>
          <fielddescription>The name of the enum type.</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ExcludedValues" columnsize="200" columntype="text" guid="26256439-bc58-415b-8586-ac6d8d592949" visible="true">
