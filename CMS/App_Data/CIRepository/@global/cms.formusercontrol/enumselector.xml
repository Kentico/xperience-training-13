          <fieldcaption>Resource prefix</fieldcaption>
          <fielddescription>
            <![CDATA[The resource prefix to use for item text localization.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="Sort" columntype="boolean" guid="397e90fe-0da9-4a26-9c14-d3511e69942b" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Sort items</fieldcaption>
          <fielddescription>Indicates if the items should be sorted.</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="SpecialItems">
        <properties>
          <caption>Special items</caption>
          <visible>true</visible>
        </properties>
      </category>
      <field allowempty="true" column="Items" columntype="longtext" guid="3db0daa4-6bb4-4366-93f6-9b9d4e8c55dc" resolvedefaultvalue="False" visible="true">
        <properties>
          <fieldcaption>Items</fieldcaption>
          <fielddescription>
            <![CDATA[List of special items in addition to the enumeration items.]]>
          </fielddescription>
        </properties>
        <settings>
          <AllowMacro>True</AllowMacro>
          <AllowQuery>True</AllowQuery>
          <controlname>optionsselector</controlname>
          <EditedFieldDataType ismacro="true">{%FieldEditorDataType%}</EditedFieldDataType>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInCustomTables>True</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>