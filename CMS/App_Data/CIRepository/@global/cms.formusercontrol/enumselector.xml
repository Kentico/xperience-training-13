          <fieldcaption>Excluded values</fieldcaption>
          <fielddescription>
            <![CDATA[Values separated by semicolon which you want exclude from selection.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="UseStringRepresentation" columntype="boolean" guid="605da75d-5017-49a3-ae12-7d17d49abff4" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Use string representation</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if string representation for enumeration items value will be used if available.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="Design">
        <properties>
          <visible>true</visible>
        </properties>
      </category>
