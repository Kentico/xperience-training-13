        <properties>
          <defaultvalue>0</defaultvalue>
          <fieldcaption>Display type</fieldcaption>
          <fielddescription>Type of the control to use for editing.</fielddescription>
        </properties>
        <settings>
          <AssemblyName>CMS.Base.Web.UI</AssemblyName>
          <controlname>enumselector</controlname>
          <Sort>False</Sort>
          <TypeName>CMS.Base.Web.UI.EnumDisplayTypeEnum</TypeName>
          <UseStringRepresentation>False</UseStringRepresentation>
        </settings>
      </field>
      <field allowempty="true" column="ResourcePrefix" columnsize="100" columntype="text" displayinsimplemode="true" guid="56675be3-b411-4c03-bb7b-64d928bfae09" visible="true">
