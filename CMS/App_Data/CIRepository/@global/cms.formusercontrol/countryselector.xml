<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>countrySelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with country names as values.]]>
  </UserControlDescription>
  <UserControlDisplayName>Country selector</UserControlDisplayName>
  <UserControlFileName>~/CMSFormControls/CountrySelector.ascx</UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>70b36f56-d293-4b46-a4f5-c3b83155ad0b</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field allowempty="true" column="ReturnType" columntype="integer" guid="e64d609e-053a-4d65-aac6-6895657ce8a4" visible="true">
        <properties>
          <fieldcaption>Return type</fieldcaption>
          <fielddescription>
            <![CDATA[Determines the type of value that will be stored by the country selector. You can choose between the country ID, state ID or both separated by a semicolon.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>radiobuttonscontrol</controlname>
          <Options>
<![CDATA[
0;{$countryselector.both$}
1;{$countryselector.country$}
2;{$countryselector.state$}
]]>
          </Options>
          <RepeatDirection>vertical</RepeatDirection>
        </settings>
      </field>
      <field allowempty="true" column="StateIDColumnName" columnsize="50" columntype="text" guid="08379b7e-17fc-41e8-bf4f-0a4e5deba154" visible="true">
        <properties>
          <fieldcaption>State ID Column Name</fieldcaption>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="AddAllItemsRecord" columntype="boolean" guid="54305825-d961-4ede-bdf7-0dc6bb845edd" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Allow to choose (all)</fieldcaption>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AddNoneRecord" columntype="boolean" guid="caa93959-ee98-4995-8375-edf0e0528137" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Allow to choose (none)</fieldcaption>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AddSelectCountryRecord" columntype="boolean" guid="62d1d00a-cb73-483d-82c2-c5ca42d43960" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Add (select country) record on the first position</fieldcaption>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="EnableStateSelection" columntype="boolean" guid="333d59e6-5a20-4df3-912d-42e07f505ef7" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Enable state selection</fieldcaption>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>100</UserControlPriority>
  <UserControlShowInCustomTables>True</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>