<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>UserSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a selector of users registered on the site. Returns user ID.]]>
  </UserControlDescription>
  <UserControlDisplayName>User selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/Membership/FormControls/Users/selectuser.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>0179994d-6a15-40b9-9594-085893858ed1</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <category name="GeneralSettings">
        <properties>
          <caption>General settings</caption>
          <visible>true</visible>
        </properties>
      </category>
      <field allowempty="true" column="SelectionMode" columntype="integer" guid="f03ad48e-559b-42dc-bb38-71cf0480c403" visible="true">
        <properties>
          <fieldcaption>Selection mode</fieldcaption>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <EditText>False</EditText>
          <Options>
<![CDATA[
0;Single text box
1;Single drop down list
2;Multiple
3;Multiple text box
4;Single button
5;Multiple button
]]>
          </Options>
        </settings>
      </field>
      <field allowempty="true" column="ReturnColumnName" columnsize="200" columntype="text" guid="675b7886-8ae9-4b66-91de-e971f4e82b8b" visible="true">
        <properties>
          <fieldcaption>Return column name</fieldcaption>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="FilterSettings">
        <properties>
          <caption>Filter settings</caption>
          <visible>true</visible>
        </properties>
      </category>
      <field allowempty="true" column="ShowSiteFilter" columntype="boolean" displayinsimplemode="true" guid="463e1902-22df-47ea-bc55-d80d42532720" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Show site filter</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether the site filter should be shown in the user selection dialog.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="HideDisabledUsers" columntype="boolean" guid="145d3c35-485a-4ba7-b2c6-c33b4b7c7de8" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Hide disabled users</fieldcaption>
          <fielddescription>
            <![CDATA[If enabled disabled users aren't shown in selector.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="HideHiddenUsers" columntype="boolean" guid="cbc91e36-36f8-4f9c-b1a1-55443d4b137c" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Hide hidden users</fieldcaption>
          <fielddescription>
            <![CDATA[If enabled hidden users aren't shown in selector (does not apply to global administrators, they are shown regardless of this property).]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="HideNonApprovedUsers" columntype="boolean" guid="792f0057-5516-48c4-9b25-c7309d0c5975" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Hide non-approved users</fieldcaption>
          <fielddescription>
            <![CDATA[If enabled non-approved users aren't shown is selector.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AlwaysShowGlobalAdministrators" columntype="boolean" guid="8b54b9c1-1cbf-4321-b65e-c88de1ef83d4" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Always show global administrators</fieldcaption>
          <fielddescription>
            <![CDATA[If enabled global administrators are shown is selector.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInCustomTables>True</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
  <UserControlShowInReports>True</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>