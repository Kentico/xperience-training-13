        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ResolverName" columnsize="500" columntype="text" guid="a714e109-09e9-4a6a-9ce3-2be479e98f79" visible="true">
