<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>ManufacturerSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with available E-commerce manufacturers.]]>
  </UserControlDescription>
  <UserControlDisplayName>Manufacturer selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/Ecommerce/FormControls/ManufacturerSelector.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>08671fd2-b9c9-4e2f-9f9b-e7085018bc56</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field allowempty="true" column="UseManufacturerNameForSelection" columntype="boolean" guid="86ab0b18-a18d-4e35-97b2-c05a493d84ca" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use manufacturer name for selection</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selector returns the code name of the selected item, instead of its ID.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AllowCreate" columntype="boolean" guid="7d9d4565-b484-4cb7-9477-d39f6784e668" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>{$com.selector.allowcreatetitle$}</fieldcaption>
          <fielddescription>{$com.selector.allowcreatedescription$}</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="EnsureSelectedItem" columntype="boolean" guid="8b62fd2f-9209-4ecb-b794-d4bf9c6caf7b" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Ensure selected item</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selected item is present regardless of other settings.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddAllItemsRecord" columntype="boolean" guid="6815beda-2d92-4026-b56b-b5e40ccf7bec" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Add all items record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (all) option for selecting all manufacturers is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddNoneRecord" columntype="boolean" guid="9279a6b6-71db-41b3-bb06-b3654aef41c5" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Add none record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (none) option is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayOnlyEnabled" columntype="boolean" guid="625e3b37-533e-4e5c-af80-1be291dd5b0e" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display only enabled</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if only enabled manufacturers are to be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="ReflectGlobalProductsUse" columntype="boolean" guid="442dd095-53a9-49e8-a7bb-bf97e4596254" visible="true">
        <properties>
          <defaultvalue>False</defaultvalue>
          <fieldcaption>Reflect use of global products</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if global items are to be displayed if global products are used on the site.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlResourceID>
    <CodeName>CMS.Ecommerce</CodeName>
    <GUID>684e021b-8b56-4cbf-8fd5-b7a791fd2dbc</GUID>
    <ObjectType>cms.resource</ObjectType>
  </UserControlResourceID>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>True</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>