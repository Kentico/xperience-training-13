<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>PublicStatusSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with available E-commerce public statuses.]]>
  </UserControlDescription>
  <UserControlDisplayName>Public status selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/ECommerce/FormControls/PublicStatusSelector.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>76b17d43-4928-408c-9e15-51d0912ef4c3</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field column="UseStatusNameForSelection" columntype="boolean" guid="18a6876a-267a-4d3e-9250-301fe4d202d8" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use status name for selection</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selector returns the code name of the selected item, instead of its ID.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AllowCreate" columntype="boolean" guid="612ee5e1-2581-42ea-af04-1b506a798452" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>{$com.selector.allowcreatetitle$}</fieldcaption>
          <fielddescription>{$com.selector.allowcreatedescription$}</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="EnsureSelectedItem" columntype="boolean" guid="82e869ea-4640-4cec-badf-8e3a6c94b4fd" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Ensure selected item</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selected item is present in the list regardless of other settings.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddAllItemsRecord" columntype="boolean" guid="ee476a45-9913-4c25-ab53-fbd0f6bc529b" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Add all items record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (all) option for selecting all public statuses is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddNoneRecord" columntype="boolean" guid="981412f2-d976-4da4-ae65-d3d89193e397" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Add none record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (none) option is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="ReflectGlobalProductsUse" columntype="boolean" guid="f3a2c3f4-fcb8-465f-9470-df9e8d197a0e" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Reflect use of global products</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if global items are to be displayed if global products are used on the site.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayOnlyEnabled" columntype="boolean" guid="bbdc710b-8a47-4806-933b-22492740b657" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display only enabled</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if only enabled public statuses are to be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlResourceID>
    <CodeName>CMS.Ecommerce</CodeName>
    <GUID>684e021b-8b56-4cbf-8fd5-b7a791fd2dbc</GUID>
    <ObjectType>cms.resource</ObjectType>
  </UserControlResourceID>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>True</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>