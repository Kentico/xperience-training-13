<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>ObjectColumnSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with columns of an object type specified in the options.]]>
  </UserControlDescription>
  <UserControlDisplayName>Object column selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSFormControls/Selectors/SelectObjectColumn.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>False</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>d1b4526a-68a3-4116-81fa-39b3ae5071dc</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field column="ObjectType" columnsize="200" columntype="text" guid="5164054e-437a-49a5-a41e-ee23dd297cac" visible="true">
        <properties>
          <fieldcaption>Object type</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the data class of the objects to be selected (e.g., cms.user).]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="ShowAllColumns" columntype="boolean" guid="6b6ae611-4a92-4514-8211-c1e8e6a83f43" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Show all columns</fieldcaption>
          <fielddescription>Indicates if all columns should be visible.</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="UseColumnCaption" columntype="boolean" guid="66ca5be6-a151-42f9-992f-093d87f594e5" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Use column caption</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if column caption should be used instead of column name.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>True</UserControlShowInReports>
  <UserControlShowInSystemTables>True</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>