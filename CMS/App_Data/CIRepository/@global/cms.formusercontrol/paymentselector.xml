<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>paymentSelector</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a drop-down list with available E-commerce types of payment.]]>
  </UserControlDescription>
  <UserControlDisplayName>Payment selector</UserControlDisplayName>
  <UserControlFileName>
    <![CDATA[~/CMSModules/ECommerce/FormControls/paymentselector.ascx]]>
  </UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>False</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>True</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>True</UserControlForText>
  <UserControlGUID>4a1246a4-fff8-4ed6-b870-56e79d6be29c</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field column="UsePaymentNameForSelection" columntype="boolean" guid="ebf34827-624a-4af7-9715-8e4772c5da4a" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Use payment name for selection</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selector returns the code name of the selected item, instead of its ID.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ShippingOptionIDColumnName" columnsize="100" columntype="text" guid="271955fe-a90e-4e9f-8c9d-10de97c6ca4a" visible="true">
        <properties>
          <fieldcaption>Shipping option ID column name</fieldcaption>
          <fielddescription>
            <![CDATA[If set, property ShippingOptionIDColumnName is set based on value of shipping option ID column value]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field column="EnsureSelectedItem" columntype="boolean" guid="9bf16a1e-cdac-4f4d-96d8-219212ebc665" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Ensure selected item</fieldcaption>
          <fielddescription>
            <![CDATA[If checked, the selected item is present in the list regardless of other settings.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="AddAllItemsRecord" columntype="boolean" guid="10ac98d4-9736-4774-bb8c-125b37a2f140" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Add all items record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (all) option for selecting all payments is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="AddNoneRecord" columntype="boolean" guid="78185677-ee6f-4cfe-a3bf-6e76dfeca660" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Add none record</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the (none) option is to be included in the list.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field column="DisplayOnlyEnabled" columntype="boolean" guid="0541ac6d-40d9-45aa-9622-159b80fbb241" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Display only enabled</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if only enabled payments are to be displayed.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlResourceID>
    <CodeName>CMS.Ecommerce</CodeName>
    <GUID>684e021b-8b56-4cbf-8fd5-b7a791fd2dbc</GUID>
    <ObjectType>cms.resource</ObjectType>
  </UserControlResourceID>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>False</UserControlShowInDocumentTypes>
  <UserControlShowInReports>False</UserControlShowInReports>
  <UserControlShowInSystemTables>False</UserControlShowInSystemTables>
  <UserControlShowInWebParts>True</UserControlShowInWebParts>
</cms.formusercontrol>