<?xml version="1.0" encoding="utf-8"?>
<cms.formusercontrol>
  <UserControlCodeName>DirectUploadControl</UserControlCodeName>
  <UserControlDescription>
    <![CDATA[Provides a tool for direct uploading of files from the file system. Can be used in Page types.]]>
  </UserControlDescription>
  <UserControlDisplayName>Direct uploader</UserControlDisplayName>
  <UserControlFileName>~/CMSFormControls/Media/DirectUploadControl.ascx</UserControlFileName>
  <UserControlForBinary>False</UserControlForBinary>
  <UserControlForBoolean>False</UserControlForBoolean>
  <UserControlForDateTime>False</UserControlForDateTime>
  <UserControlForDecimal>False</UserControlForDecimal>
  <UserControlForDocAttachments>False</UserControlForDocAttachments>
  <UserControlForDocRelationships>False</UserControlForDocRelationships>
  <UserControlForFile>True</UserControlForFile>
  <UserControlForGuid>False</UserControlForGuid>
  <UserControlForInteger>False</UserControlForInteger>
  <UserControlForLongText>False</UserControlForLongText>
  <UserControlForText>False</UserControlForText>
  <UserControlGUID>1429d205-5432-43ea-bace-0ae1eb45fca3</UserControlGUID>
  <UserControlIsSystem>False</UserControlIsSystem>
  <UserControlParameters>
    <form version="2">
      <field allowempty="true" column="Extensions" columnsize="10" columntype="text" displayinsimplemode="true" guid="7d900199-ba63-4d53-bc3e-2fe6645c1515" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Allowed extensions</fieldcaption>
          <fielddescription>
            <![CDATA[Only files using the extensions specified here can be uploaded into the field. Multiple extensions must be separated by semicolons. By default, the list of allowed extensions is inherited from the Administration -> Settings -> System -> Files -> Upload extensions setting.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>allowedextensionsselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Allowed_Extensions" columnsize="250" columntype="text" guid="7b3f2414-ce38-4bf3-8296-cfc68799b61d" spellcheck="false">
        <settings>
          <controlname>agerangeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize" columntype="longtext" guid="469a8949-ed96-402a-b4b8-d5f6839ff9f3" spellcheck="false" visible="true">
        <properties>
          <fieldcaption>Automatic image resize on upload</fieldcaption>
          <fielddescription>
            <![CDATA[The resizing settings specified here are applied to all uploaded image-type files.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>autoresizeconfiguration</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_Width" columntype="integer" guid="2db9e2fc-60f5-4d85-8a7f-29525a57ed36" spellcheck="false">
        <settings>
          <controlname>badgeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_Height" columntype="integer" guid="cbb80827-0f12-4638-8254-6689519c2b0a" spellcheck="false">
        <settings>
          <controlname>badgeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_MaxSideSize" columntype="integer" guid="65122dc6-6b0f-4c4a-8706-5c44f4590e6c" spellcheck="false">
        <settings>
          <controlname>badgeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Autoresize_Hashtable" columntype="boolean" guid="b5e02ba5-7ef7-4f08-aad9-12d877aee0a1" spellcheck="false">
        <properties>
          <defaultvalue>true</defaultvalue>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
  </UserControlParameters>
  <UserControlPriority>0</UserControlPriority>
  <UserControlShowInCustomTables>False</UserControlShowInCustomTables>
  <UserControlShowInDocumentTypes>True</UserControlShowInDocumentTypes>
