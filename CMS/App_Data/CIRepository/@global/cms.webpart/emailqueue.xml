          <fielddescription>{$documentation.webpartproperties.docperpage$}</fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <options>
            <![CDATA[<item value="10" text="10" /><item value="25" text="25" /><item value="50" text="50" />]]>
          </options>
