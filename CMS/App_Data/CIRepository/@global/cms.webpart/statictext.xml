  <WebPartProperties>
    <form version="2">
      <category name="Content">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="Text" columntype="longtext" guid="dda567a5-f65a-4405-80ee-884b3d7ce0a6" translatefield="true" visible="true">
        <properties>
          <fieldcaption>Text</fieldcaption>
          <fielddescription>Used to enter the text that will be displayed.</fielddescription>
        </properties>
        <settings>
          <controlname>largetextarea</controlname>
        </settings>
      </field>
      <field allowempty="true" column="Tag" columnsize="200" columntype="text" guid="85158c87-b28a-4af0-bd65-08961c728e65" visible="true">
        <properties>
          <fieldcaption>Display as</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies as which HTML tag the content text will be rendered.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <Options>
<![CDATA[
;(plain text)
h1;Header level 1
h2;Header level 2
h3;Header level 3
p;Paragraph
div;Block
pre;Preformatted text
]]>
          </Options>
        </settings>
      </field>
      <field allowempty="true" column="EncodeText" columntype="boolean" guid="abc5de63-8c1c-4b97-baaa-e965a434e408" spellcheck="false" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Encode text</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates whether the entered text should be HTML encoded.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ResolveDynamicControls" columntype="boolean" guid="d709a046-8342-4f42-bf69-c2970e0a9805" visible="true">
        <properties>
          <defaultvalue>true</defaultvalue>
          <fieldcaption>Resolve dynamic controls</fieldcaption>
          <fielddescription>
            <![CDATA[Indicates if the code of any inline controls entered into the text such as BizForms, polls, etc. should be resolved.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
    </form>
