          <fielddescription>
            <![CDATA[{$documentation.webpartproperties.cachedependencies$}]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>cachedependencies</controlname>
        </settings>
      </field>
      <category name="Hidden data properties">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
