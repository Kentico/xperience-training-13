<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>Text</CodeName>
    <GUID>68d8e65b-a01c-49ae-b9b6-7580bbce4b09</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[Displays a hyperlink to a specified target URL. The link can be rendered either in text format or as an image.]]>
  </WebPartDescription>
  <WebPartDisplayName>Link / Button</WebPartDisplayName>
  <WebPartFileName>Text/Link.ascx</WebPartFileName>
  <WebPartGUID>734f79fc-7815-4b8c-bbce-52f85d50f66e</WebPartGUID>
  <WebPartIconClass>icon-w-link-button</WebPartIconClass>
  <WebPartName>Link</WebPartName>
  <WebPartProperties>
    <form version="2">
      <category name="Content">
        <properties>
          <visible>True</visible>
        </properties>
      </category>
      <field allowempty="true" column="LinkText" columnsize="500" columntype="text" guid="30a4b79f-bedb-4263-92b4-270730b6a980" visible="true">
        <properties>
          <fieldcaption>Link text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the text of the link. If empty, the URL will be used.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="LinkCssClass" columnsize="200" columntype="text" guid="b6850bc0-0bc0-4bc9-93d3-9faea88faacf" visible="true">
        <properties>
          <fieldcaption>Link CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the name of the CSS class (from the currently used CSS stylesheet) that will be applied to the link text.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ShowAsButton" columntype="boolean" guid="f1eab810-3fd2-426f-8be5-98236f2a9e8b" visible="true">
        <properties>
          <defaultvalue>false</defaultvalue>
          <fieldcaption>Show as button</fieldcaption>
          <fielddescription>If checked, the link is shown as a button.</fielddescription>
        </properties>
        <settings>
          <controlname>checkboxcontrol</controlname>
        </settings>
      </field>
      <category name="Image">
        <properties>
          <visible>true</visible>
        </properties>
      </category>
      <field allowempty="true" column="ImageUrl" columnsize="200" columntype="text" guid="c8c8bd83-f293-4d90-9040-b2a4ac97b109" resolvedefaultvalue="False" visible="true">
        <properties>
          <fieldcaption>Image URL</fieldcaption>
          <fielddescription>
            <![CDATA[Sets the URL of an image that will be used as the link. You can enter either an absolute or relative URL. Absolute URLs need to be entered including their protocol prefix (e.g. https://xperience.io). Images stored as part of your website can be conveniently selected by clicking the Select button.]]>
          </fielddescription>
        </properties>
        <settings>
          <Autoresize_Hashtable>True</Autoresize_Hashtable>
          <controlname>mediaselectioncontrol</controlname>
          <ShowPreview>True</ShowPreview>
        </settings>
      </field>
      <field allowempty="true" column="ImageAltText" columnsize="200" columntype="text" guid="38a5b3e7-9f9c-402e-b32f-0bbc94d66ba3" visible="true">
        <properties>
          <fieldcaption>Image alternate text</fieldcaption>
          <fielddescription>
            <![CDATA[Sets a tooltip for the link image (the value of the "alt" attribute of the <img> tag).]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="ImageCssClass" columnsize="200" columntype="text" guid="e7fcbaa3-f454-4aaa-ae32-322e3b921e72" visible="true">
        <properties>
          <fieldcaption>Image CSS class</fieldcaption>
          <fielddescription>
            <![CDATA[Specifies the name of the CSS class (from the currently used CSS stylesheet) that will be applied to the link image.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <category name="Action">
        <properties>
          <visible>true</visible>
        </properties>
      </category>
      <field allowempty="true" column="LinkUrl" columnsize="2000" columntype="text" guid="b7f25ed5-c9a2-41a8-9d98-e1711f251fc5" resolvedefaultvalue="False" visible="true">
        <properties>
          <fieldcaption>Link URL</fieldcaption>
          <fielddescription>
            <![CDATA[If set, the link points to a particular URL. You can enter either absolute or relative URLs. Absolute URLs need to be entered including their protocol prefix (e.g. https://xperience.io). Link targets within your website can be conveniently selected by clicking the Select button.]]>
          </fielddescription>
        </properties>
        <settings>
          <Autoresize>
<![CDATA[
<AutoResize>
  <autoresize>noresize</autoresize>
  <autoresize_width />
  <autoresize_height />
  <autoresize_maxsidesize />
</AutoResize>
]]>
          </Autoresize>
          <Configuration>True</Configuration>
          <controlname>urlselector</controlname>
          <Dialogs_Attachments_Hide>True</Dialogs_Attachments_Hide>
          <Dialogs_Content_Hide>False</Dialogs_Content_Hide>
          <Dialogs_Content_Site>##all##</Dialogs_Content_Site>
          <Dialogs_Libraries_Site>##all##</Dialogs_Libraries_Site>
        </settings>
      </field>
      <field allowempty="true" column="LinkTarget" columnsize="100" columntype="text" guid="fb3d135a-c9e9-4231-8521-81c2e816f0d9" visible="true">
        <properties>
          <fieldcaption>Link target</fieldcaption>
          <fielddescription>Sets the target window of the link.</fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="LinkEvent" columnsize="200" columntype="text" guid="8856af3e-7622-4efb-80eb-c8ffebdde6d0" visible="true">
        <properties>
          <fieldcaption>Raise event</fieldcaption>
          <fielddescription>
            <![CDATA[If set, the action raises the postback event on the page.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <EditText>True</EditText>
          <Options>
<![CDATA[
Submit;Submit
Previous;Previous
Next;Next
]]>
          </Options>
        </settings>
      </field>
      <field allowempty="true" column="LinkJavascript" columntype="longtext" guid="55292811-2250-42ef-962c-dddc56452bb1" visible="true">
        <properties>
          <fieldcaption>Link javascript</fieldcaption>
          <fielddescription>
            <![CDATA[Javascript that is executed upon clicking on the link.]]>
          </fielddescription>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textareacontrol</controlname>
          <FilterMode>False</FilterMode>
          <IsTextArea>True</IsTextArea>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>