<?xml version="1.0" encoding="utf-8"?>
<cms.webpart>
  <WebPartCategoryID>
    <CodeName>OnlineMarketing</CodeName>
    <GUID>9ef17beb-dbe3-4e94-81ed-d161e1b9542c</GUID>
    <ObjectType>cms.webpartcategory</ObjectType>
  </WebPartCategoryID>
  <WebPartDefaultValues>
    <form />
  </WebPartDefaultValues>
  <WebPartDescription>
    <![CDATA[This web part shows a list of activities, i.e. actions such as visiting a page, making a purchase, subscribing to an event etc., performed by a contact on the website.]]>
  </WebPartDescription>
  <WebPartDisplayName>Activities</WebPartDisplayName>
  <WebPartFileName>OnlineMarketing/Activities.ascx</WebPartFileName>
  <WebPartGUID>863b4f45-f785-4c22-a09c-857c8e04569c</WebPartGUID>
  <WebPartIconClass>icon-w-activities</WebPartIconClass>
  <WebPartName>Activities</WebPartName>
  <WebPartProperties>
    <form version="2">
      <field allowempty="true" column="Site" columnsize="250" columntype="text" guid="a8374485-344b-4c78-82e7-5a1a599170a5" visible="true">
        <properties>
          <fieldcaption>Site</fieldcaption>
          <fielddescription>Selects activities from a particular site.</fielddescription>
        </properties>
        <settings>
          <controlname>siteselectorwithallfieldforglobaladmin</controlname>
        </settings>
      </field>
      <field allowempty="true" column="ActivityType" columnsize="250" columntype="text" guid="3250555b-6222-489a-b53c-353d2b380cac" visible="true">
        <properties>
          <fieldcaption>Activity type</fieldcaption>
          <fielddescription>Selects a particular activity type.</fielddescription>
        </properties>
        <settings>
          <controlname>activitytypeselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="DateInterval" columntype="integer" guid="1d166d8e-2427-4b52-856b-b54958f45e99" visible="true">
        <properties>
          <defaultvalue>0</defaultvalue>
          <fieldcaption>Date interval</fieldcaption>
          <fielddescription>
            <![CDATA[List activities newer than the specified number of days. The interval period is counted up to the present day.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>dateintervalselector</controlname>
        </settings>
      </field>
      <field allowempty="true" column="PageSize" columntype="integer" guid="5b9542a3-0a20-48ff-b1aa-2134a0e77b63" visible="true">
        <properties>
          <defaultvalue>25</defaultvalue>
          <fieldcaption>Page size</fieldcaption>
          <fielddescription>
            <![CDATA[Determines the maximum number of records displayed per page.]]>
          </fielddescription>
        </properties>
        <settings>
          <controlname>dropdownlistcontrol</controlname>
          <Options>
<![CDATA[
10;10
25;25
50;50
100;100
-1;(all)
]]>
          </Options>
        </settings>
      </field>
    </form>
  </WebPartProperties>
  <WebPartResourceID>
    <CodeName>CMS.OnlineMarketing</CodeName>
    <GUID>bd03e629-e677-42a6-8b12-5277fa04add7</GUID>
    <ObjectType>cms.resource</ObjectType>
  </WebPartResourceID>
  <WebPartSkipInsertProperties>False</WebPartSkipInsertProperties>
  <WebPartType>0</WebPartType>
</cms.webpart>