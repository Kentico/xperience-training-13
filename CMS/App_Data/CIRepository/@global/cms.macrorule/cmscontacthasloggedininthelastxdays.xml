<?xml version="1.0" encoding="utf-8"?>
<cms.macrorule>
  <MacroRuleAvailability>2</MacroRuleAvailability>
  <MacroRuleCondition>{_perfectum}Contact.LoggedIn(ToInt({days}))</MacroRuleCondition>
  <MacroRuleDescription>
    <![CDATA[{$macrorule.cmscontacthasloggedininthelastxdays.description$}]]>
  </MacroRuleDescription>
  <MacroRuleDisplayName>Contact has logged in in the last X days.</MacroRuleDisplayName>
  <MacroRuleEnabled>True</MacroRuleEnabled>
  <MacroRuleGUID>1c250b40-34be-4994-927f-6c1fb74fa05b</MacroRuleGUID>
  <MacroRuleIsCustom>False</MacroRuleIsCustom>
  <MacroRuleName>CMSContactHasLoggedInInTheLastXDays</MacroRuleName>
  <MacroRuleParameters>
    <form version="2">
      <field allowempty="true" column="days" columntype="integer" guid="679ea042-ca59-453d-bc92-1709d60693e5" visible="true">
        <properties>
          <defaultvalue>0</defaultvalue>
          <fieldcaption>enter days</fieldcaption>
        </properties>
        <settings>
          <AutoCompleteEnableCaching>False</AutoCompleteEnableCaching>
          <AutoCompleteFirstRowSelected>False</AutoCompleteFirstRowSelected>
          <AutoCompleteShowOnlyCurrentWordInCompletionListItem>False</AutoCompleteShowOnlyCurrentWordInCompletionListItem>
          <controlname>textboxcontrol</controlname>
          <FilterMode>False</FilterMode>
          <Trim>False</Trim>
        </settings>
      </field>
      <field allowempty="true" column="_perfectum" columnsize="100" columntype="text" guid="3b04aab0-b1a2-4c68-9236-fd5024aa36a0" visible="true">
        <properties>
          <defaultvalue>;has</defaultvalue>
          <fieldcaption>select operation</fieldcaption>
        </properties>
        <settings>
          <controlname>macronegationoperator</controlname>
          <Options>
<![CDATA[
;has
!;has not
]]>
          </Options>
          <RepeatDirection>horizontal</RepeatDirection>
        </settings>
      </field>
    </form>
  </MacroRuleParameters>
  <MacroRuleRequiresContext>False</MacroRuleRequiresContext>
  <MacroRuleResourceName>cms.onlinemarketing</MacroRuleResourceName>
  <MacroRuleText>
    <![CDATA[Contact {_perfectum} logged in in the last {days} day(s)]]>
  </MacroRuleText>
</cms.macrorule>