  <QueryConnectionString>CMSConnectionString</QueryConnectionString>
  <QueryGUID>56a70d47-984b-47be-8b2b-cea7122b54a2</QueryGUID>
  <QueryIsCustom>False</QueryIsCustom>
  <QueryIsLocked>False</QueryIsLocked>
  <QueryName>getactiveserverids</QueryName>
  <QueryRequiresTransaction>False</QueryRequiresTransaction>
  <QueryText>
    <![CDATA[SELECT CMS_SMTPServer.ServerID FROM CMS_SMTPServer INNER JOIN CMS_SMTPServerSite ON CMS_SMTPServer.ServerID = CMS_SMTPServerSite.ServerID WHERE ServerEnabled = 1 AND SiteID = @SiteID ORDER BY CMS_SMTPServer.ServerID]]>
  </QueryText>
  <QueryTypeID>0</QueryTypeID>
</cms.query>