  <QueryIsCustom>False</QueryIsCustom>
  <QueryIsLocked>False</QueryIsLocked>
  <QueryName>selectsitesperculture</QueryName>
  <QueryRequiresTransaction>False</QueryRequiresTransaction>
  <QueryText>
    <![CDATA[SELECT S.* FROM CMS_Site S, CMS_Culture C, CMS_SiteCulture SC WHERE C.CultureCode LIKE @CultureCode AND SC.CultureID = C.CultureID AND S.SiteID = SC.SiteID]]>
  </QueryText>
  <QueryTypeID>0</QueryTypeID>
</cms.query>